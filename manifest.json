{
  "manifest_version": 2,
  "name": "RandSearch",
  "version": "1.0",
  "background": { "scripts": ["background.js"] },
  "browser_action": {"default_icon": "icon.png"},
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery.js", "content.js"]
    }
  ]
}
